 	     ################# PEAK FLOW METER Description ############## 
 	     #################################################################################### -->
 
  <owl:Class rdf:ID="PeakExpiratoryFlow">		
		<rdfs:label xml:lang="en">Peak Expiratory Flow (PEF)</rdfs:label>
		<rdfs:comment xml:lang="en">The peak expiratory flow (PEF), also called peak expiratory flow rate (PEFR) is a person's maximum speed of expiration, as measured with a peak flow meter, a small, hand-held device used to monitor a person's ability to breathe out air. Peak expiratory flow is typically measured in units of liters per minute (L/min). Useful for kHealth projects (asthma, bariatric surgery, etc.) at Knoesis.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-06-29</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-27</dcterms:issued>
	</owl:Class>
	
